%%% Dependence of time to extinction on foraging parameters

global k B D;

% choose parameters
h = 0.01;  % handling time
alpha = 0; % attack rate

lambda = 1.5;
mu = 0.5;
r = lambda-mu;
q = 0;
k = 150; 
n = 1:k;

% birth and death rates, see eq.(3)
B = lambda*n.*(1-n/k);
D = mu*n+alpha*n.^(q+1)./(1+alpha*h*n.^(q+1));

qs = iterative_qs();
plot(qs);
hold on;
plot(zeros(1,k),'--k');
plot((B-D)/1000,'-k');
hold off;

mteq = 1/(D(1)*qs(1))
